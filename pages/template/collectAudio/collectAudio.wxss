.audio-template{
  display: flex;
  justify-content: row;
  align-items: center;
  /* padding: 10px 0px; */
  border-bottom: 1px solid #E6E7EB;
  background-color: #ffffff;
  height: 80px; 
}
.audio-template:active{
  background-color: #E6E7EB;
}
.audio-template .image{
  width: 100px;
  height: 68px;
  margin-left: 1vw;
  border-radius:10px;

  /* padding: 4px 0px */
}
.audio-template .audio-info{
  display: flex;
  flex-direction: column;
  margin-left: 1vw;
  justify-content: space-between;
  width: 100%;
  height: 68px;

}
.audio-template .img-play-num{
  width: 12px;
  height: 12px;
}
.audio-template .audio-num{
  display: flex;
  width: 190px;
  justify-content: space-between
}
.audio-template .img-rate-num{
  width: 12px;
  height: 12px;
}
.story-count{
  font-size: 12px;
  color: #999;
}
.audio-template .audio-num-play{
    display: flex;
    justify-content: flex-start;
    width: 80px;
    align-items: center;
}
.audio-num-play .right{
  margin-left: 2px;
  color: #999;
  font-size:12px;
}
.audio-template .audio-num-rate{
    display: flex;
    justify-content: flex-start;
    width: 80px;
    align-items: center;
}
.audio-num-rate .right{
  margin-left: 2px;
  color: #999;
  font-size:12px;
}